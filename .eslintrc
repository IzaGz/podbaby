{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "settings": {
        "ecmascript": 7,
        "jsx": true
    },
    "plugins": [
        //"react"
    ],
    "builtins: {
        "window": true
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "camelcase": 0,
        "semi": 1,
        "no-unused-vars": 1,
        "no-underscore-dangle": 0,
        "no-comma-dangle": 1
    }
}
